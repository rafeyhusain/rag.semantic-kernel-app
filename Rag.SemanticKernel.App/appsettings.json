{
    "Log": {
        "FileName": "log.txt"
    },
    "Elasticsearch": {
        "Url": "http://localhost:9200",
        "IndexName": "memory-service"
    },
    "AiService": {
        "ApiKey": "your-ai-service-key",
        "EmbeddingModel": "mistral-embed",
        "LanguageModel": "mistral-large-latest"
    },
    "EmbeddingModels": [
        {
            "Name": "MistralEmbedDefault",
            "DimensionSize": 1024,
            "CostPerMillion": 0.1,
            "Latency": 50,
            "IsDefault": true
        },
        {
            "Name": "OpenAIEmbedding",
            "DimensionSize": 1536,
            "CostPerMillion": 0.2,
            "Latency": 75,
            "IsDefault": false
        }
    ],
    "SemanticSearch": {
        "RelevanceThreshold": 0.5,
        "MaxResults": 5,
        "EnableMetadataFiltering": true,
        "EnableHybridSearch": true
    }
}